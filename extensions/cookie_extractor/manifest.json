{
  "manifest_version": 3,
  "name": "Gemini Business Cookie Extractor",
  "version": "1.0.0",
  "description": "Extract cookies and account info from Gemini Business",
  "permissions": [
    "cookies",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://business.gemini.google/*",
    "https://*.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://business.gemini.google/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}